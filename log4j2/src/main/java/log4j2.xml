<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="off">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %l %-5level - %msg%n" />
		</Console>
	</Appenders>
	
	<Loggers>
		<Logger name="org.marceloleite.log4jexample.CheckPrimeRangeCallable" level="trace"/>
		<Root level="info">
			<AppenderRef ref="Console" />
		</Root>
	</Loggers>
</Configuration>